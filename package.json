{
  "name": "react-cmf",
  "description": "A framework built on top of best react libraries",
  "main": "lib/index.js",
  "scripts": {
    "docs:js": "jsdoc -c docs.json",
    "docs:cleanup": "rm -rf docs/jsodc/*",
    "docs": "npm run docs:cleanup && npm run docs:js",
    "prepublish": "babel -d lib ./src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint:es": "eslint --config .eslintrc --ext .js src",
    "lint": "npm run lint:es",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-cmf.git"
  },
  "keywords": [
    "react",
    "framework"
  ],
  "author": "Jean-Michel FRANCOIS <jmfrancois@talend.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Talend/react-cmf/issues"
  },
  "homepage": "https://github.com/Talend/react-cmf#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "classnames": "2",
    "coveralls": "2.11.14",
    "enzyme": "2.5.1",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "immutable": "3.8.1",
    "ink-docstrap": "^1.3.0",
    "invariant": "2.2.1",
    "jasmine-immutable-matchers": "1.0.1",
    "jest": "16.0.2",
    "jest-cli": "16.0.2",
    "jsdoc": "^3.4.1",
    "react": "15",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15",
    "react-redux": "4",
    "react-router": "^3.0.0",
    "react-router-redux": "4",
    "react-test-renderer": "15.3.2",
    "redux": "3",
    "redux-logger": "2.7.0",
    "redux-mock-store": "^1.1.4",
    "redux-thunk": "2",
    "semantic-release": "4.3.5"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  },
  "peerDependencies": {
    "classnames": "2",
    "immutable": "3",
    "invariant": "2",
    "react": "15",
    "react-dom": "15",
    "react-redux": "4",
    "react-router": "2",
    "react-router-redux": "4",
    "redux": "3",
    "redux-thunk": "2"
  }
}
