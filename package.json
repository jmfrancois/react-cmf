    "eslint-plugin-react": "6.2.0",
    "immutable": "3.8.1",
    "invariant": "2.2.1",
    "jest": "14.1.0",
    "jest-cli": "14.1.0",
    "react": "15",
    "react-addons-test-utils": "15.3.1",
    "react-dom": "15",
    "react-redux": "4",
    "react-router": "2",
    "react-router-redux": "4",
